.row
  .small-12
    %h3.title.margin-top-30
      = I18n.t(:titleInterests) 
.row.padding-h-30.padding-top-30
  .small-12.columns.padding-left-40.padding-right-40.padding-bottom-30
    .row.font-medium
      %ul
        - I18n.t('interests').each do |categorie|
          - categorie[:names].each do |name|
            %li.font-medium
              %i.fa.margin-right-10.grey-5{class: "fa-#{categorie[:meta]}"}
              #{name}